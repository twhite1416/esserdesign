/****************************************|TYPOGRAPHY|****************************************/
body {
	min-width: 780px;
	min-width: 320px;

	background: url(/assets/images/bg-white.png);

	color: #a5a5a5;
	font-family: myriad-pro,sans-serif;
	font-size: 14px;
	line-height: 20px;
}

h1,h2,h3,h4{font-weight:400;line-height:1em;}
h1{font-size:48px;font-weight:bold;text-transform:lowercase;text-shadow:-1px 0 0 rgba(255,255,255,1.0)}
h2{margin:0 0 16px;font-size:28px;line-height:41px;/*letter-spacing:.03em;*/}	
h3{font-size:20px;}
h4{font-size:18px;}
	h2.bold,span.bold,h2 bold,h2 span{font-weight: 400;}
	h1 a,h1 a:hover,h2 a,h2 a:hover,h3 a,h3 a:hover,h4 a,h4 a:hover{color:inherit;text-decoration:none;}


ol, ul {
	list-style: none;
}
blockquote, q {
	quotes: none;
}
.pad {
	padding: 0 20px;
}
blockquote:before, blockquote:after, q:before, q:after {
	content: '';
	content: none;
}
a {
	color: #f4b213;
	text-decoration: none;
}
a:hover {
	text-decoration: underline;
}
a, a:visited, a:hover, a:active, a:focus {
	-moz-outline-style: none;
	cursor: pointer;
}
ins {
	text-decoration: none;
}
del {
	text-decoration: line-through;
}
#content img {
	border: 2px solid #FFFFFF;
}
.more {
	text-align: right;
}
.more a {
	color: #903;
	text-decoration: none;
	text-transform: lowercase;
	font-weight: bold;
}
