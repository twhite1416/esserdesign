/*Better Size*/
@media only screen and (min-width: 1000px) {
}
@media only screen and (max-height: 850px) {
	#hgroup{height:60px;}
	#header h1{top:7px;}
	#navigation{top:19px;}
	#header h1 a{background-position:0 -180px;}
	#strip{height:64px;overflow:hidden;}
	#main{padding-top:80px;}
	#main2{padding-top:90px;}
	
	#masthead{margin-bottom:70px;}
	.article .header{padding:0 0 12px;}
	.article .section{padding-top:0;}
}
@media only screen and (max-height: 700px) {
	#hgroup {
		height: 55px;
	}
	#header h1 {
		top: 4px;
	}
	#header h1 a {
