/*GRID*/
.projects,.projects .frame,.projects .grid,.projects .featured {
	position: relative;
}
#projects header{padding:0 0 12px;overflow:hidden;}
#projects .header h1 {float:left;}
#projects h2,#main2 h2{padding:0 3px;text-align:justify;}

.projects .frame {
	margin: 0 auto;
	height: 700px;
	width: 800px;
	overflow: hidden;
}
.projects .grid {
	/*position: absolute;
	top: 0;
	left: 0;*/
	width: 800px;
	z-index: 1;
}
.projects .grid .item {
	position: relative;
	float: left;
	margin: 5px;
	height: 150px;
	width: 150px;
	cursor: pointer;
}
.projects .grid .item .img {
	position: absolute;
	top: 0;
	left: 0;
	background: url(/assets/images/logo_ani.png);
	
}
.projects .grid .item div.img {
	padding: 25px 10px;
	height: 100px;
	width: 130px;

}
.projects .grid .item div.img span {
	display: block;
	font-size: 16px;
	line-height: 16px;
	text-align: left;
	
}
#main .projects .grid .item div.img span {
	color: #FFF;
}
.projects .grid .item div.img .name {
	font-weight: bold;
}
.projects .grid .item div.img .project {
	font-weight: 300;
}

.projects .grid .item .container{position:relative;margin:0 auto;width:100%;height:100%;z-index:1;}
.projects .grid .item .container{-webkit-perspective:1200px;-moz-perspective:1200px;-o-perspective:1200px;perspective:1200px;
  -webkit-transform: transform3d(0, 0, 0);
  -moz-transform: transform3d(0, 0, 0);
  -o-transform: transform3d(0, 0, 0);
  transform: transform3d(0, 0, 0);
}
.projects .grid .item .container .card{
  width: 100%;
  height: 100%;
  -webkit-transform-style: preserve-3d;
  -moz-transform-style: preserve-3d;
  -o-transform-style: preserve-3d;
  transform-style: preserve-3d;
  -webkit-transition: all 800ms cubic-bezier(0.770, 0.000, 0.175, 1.000);
  -moz-transition: all 800ms cubic-bezier(0.770, 0.000, 0.175, 1.000);
  -o-transition: all 800ms cubic-bezier(0.770, 0.000, 0.175, 1.000);
  transition: all 800ms cubic-bezier(0.770, 0.000, 0.175, 1.000);
}
.projects .grid .item .container:hover .card {
  -webkit-transform: rotateY(180deg) translateZ(1px);
  -moz-transform: rotateY(180deg) translateZ(1px);
  -o-transform: rotateY(180deg) translateZ(1px);
  transform: rotateY(180deg) translateZ(1px);
}
.face {
  position: absolute;
  width: 100%;
  height: 100%;
  -webkit-backface-visibility: hidden;
  -moz-backface-visibility: hidden;
  -o-backface-visibility: hidden;
  backface-visibility: hidden;
}
.face.front,{z-index:2;}
.face.front img{position:relative;z-index:2;}
.face.back {
	z-index:1;
  display: block;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  -o-box-sizing: border-box;
  box-sizing: border-box;
  -webkit-transform: rotateY(180deg) translateZ(1px);
  -moz-transform: rotateY(180deg) translateZ(1px);
  -o-transform: rotateY(180deg) translateZ(1px);
  transform: rotateY(180deg) translateZ(1px);
  padding: 10px;
  background-color: #CCC;
  color:#000;
  text-transform:lowercase;
}

.face.back h3{padding:0 25px 0 0;}
.face.back p{position:absolute;width:126px;bottom:20px;padding:10px 0 0;background:url('/assets/images/divider.gif') top left repeat-x;}

